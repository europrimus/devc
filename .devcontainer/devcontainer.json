{
    "name": "devc",
    "dockerComposeFile": "docker-compose.yml",
    "service": "app",
    "extensions": [
        "golang.go",
        "ms-azuretools.vscode-docker"
    ],
    "settings": {
        "go.formatTool": "goimports"
    },
    "vimExtensions": [
        "fatih/vim-go"
    ],
    "vimSettings": [
	    "let g:go_fmt_command = 'goimports'"
    ],
    "workspaceFolder": "/workspace"
}
